{
    "name": "faustbrian/laravel-repository",
    "description": "Repositories for Laravel & Eloquent",
    "keywords": ["laravel", "repository", "eloquent", "model", "cache"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6 || ^7.0",
        "illuminate/http": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/config": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/database": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/pagination": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/console": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/filesystem": "5.1.* || 5.2.* || 5.3.*",
        "faustbrian/laravel-validator": "^1.0",
        "faustbrian/eloquent-presenter": "^2.0",
        "spatie/laravel-fractal": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Repository\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
